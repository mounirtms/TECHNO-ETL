  "dependencies": {
    "activedirectory2": "^2.2.0",
    "apicache": "^1.6.3",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.10.2",
    "compression": "^1.8.1",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.1",
    "fs-extra": "^11.3.0",
    "got": "^14.4.7",
    "helmet": "^7.2.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "keyv": "^4.5.3",
    "morgan": "^1.10.0",
    "msgpackr": "^1.11.4",
    "mssql": "^11.0.1",
    "node-cron": "^4.0.5",
    "rate-limiter-flexible": "^7.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "nodemon": "^3.0.2",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.12",
    "webpack": "^5.98.0",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "start": "node --no-warnings --experimental-modules dist/index.js",
    "start:dev": "nodemon server.js",
    "build": "npm run clean && webpack --mode production",
    "dev": "NODE_ENV=development PORT=5000 nodemon server.js",
    "build:prod": "npm run clean && NODE_ENV=production webpack --mode production",
    "build:dev": "npm run clean && NODE_ENV=development webpack --mode development",
    "clean": "rimraf dist",
    "worker": "node worker.js",
    "flush": "redis-cli --scan --pattern 'cache:*' | xargs -r redis-cli del",
    "test": "echo 'Backend server test' && curl http://localhost:5000/api/health || echo 'Server not running'",
    "health": "curl http://localhost:5000/api/health",
    "stop": "taskkill /F /IM node.exe /FI \"WINDOWTITLE eq Techno ETL Backend*\" 2>nul || echo 'No server process found'"
  }
}
