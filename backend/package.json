  "dependencies": {
    "axios": "^1.9.0",
    "compression": "^1.8.1",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "fs-extra": "^11.3.0",
    "got": "^14.4.7",
    "helmet": "^7.2.0",
    "ioredis": "^5.4.1",
    "jest-worker": "^29.7.0",
    "joi": "^17.13.3",
    "keyv": "^5.4.0",
    "@keyv/redis": "^5.0.0",
    "morgan": "^1.10.0",
    "mssql": "^11.0.1",
    "p-limit": "^5.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "geoip-lite": "^1.4.10",
    "useragent": "^2.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "terser-webpack-plugin": "^5.3.9"
  },
  "scripts": {
    "start": "node --no-warnings --expose-gc server.js",
    "start:simple": "node --no-warnings simple-server.js",
    "start:prod": "node --no-warnings dist/server.js",
    "dev": "NODE_ENV=development PORT=5000 nodemon --expose-gc server.js",
    "dev:simple": "NODE_ENV=development PORT=5000 nodemon simple-server.js",
    "build": "node build.js",
    "build:prod": "cross-env NODE_ENV=production node build.js",
    "cron": "node --no-warnings cron-runner.js",
    "cron:prod": "node --no-warnings dist/cron-runner.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:reload": "pm2 reload techno-etl-api",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "flush": "redis-cli --scan --pattern 'cache:*' | xargs -r redis-cli del",
    "health": "curl http://localhost:5000/api/health",
    "metrics": "curl http://localhost:5000/api/metrics",
    "test": "echo 'Backend server test' && curl http://localhost:5000/api/health || echo 'Server not running'",
    "stop": "taskkill /F /IM node.exe /FI \"WINDOWTITLE eq Techno ETL Backend*\" 2>nul || echo 'No server process found'",
    "clean": "rimraf dist node_modules/.cache",
    "prebuild": "npm run clean"
  }
}
