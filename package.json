  "scripts": {
    "start": "vite --host 0.0.0.0 --port 80",
    "server": "cd backend && npm run dev",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "build": "npm run type-check && npm run validate:env && vite build --mode production",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:debug": "vite build --mode development --minify false --sourcemap true",
    "preview:safe": "npm run build && npx serve -s dist -p 3000 --cors",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "fix:contexts": "node fix-context-bundling.js && npm run build:debug",
    "backend:build": "cd backend && node build-optimized.js",
    "backend:start": "cd backend/dist && npm run start:cluster",
    "fix": "node fix-react-errors.js",
    "fix:context": "node fix-react-context.js",
    "quick-fix": "node quick-fix-context.js && npm run build",
    "deploy": "node deploy-optimized.js",
    "deploy:build": "node deploy-optimized.js build",
    "deploy:health": "node deploy-optimized.js health",
    "deploy:check-ports": "node deploy-optimized.js check-ports",
    "start:dev": "node start-dev-optimized.js",
    "start:dev:legacy": "concurrently \"npm run dev\" \"npm run backend:dev\"",
    "start:prod": "concurrently \"npm run preview\" \"npm run backend:start\"",
    "clean": "rimraf dist_new backend/dist",
    "test": "npm run build && npm run preview",
    "info": "echo üöÄ TECHNO-ETL v2.1.0 - Fixed and Optimized && echo üë®‚Äçüíª Author: Mounir Abderrahmani && echo üìß Email: mounir.ab@techno-dz.com",
    "validate:env": "node validate-environment.js",
    "apply:tunings": "node apply-project-tunings.js",
    "dev:optimized": "concurrently \"npm run start\" \"npm run server\" \"npm run validate:env\"",
    "optimize": "node optimize-frontend.js",
    "build:optimized": "npm run optimize && npx vite build --mode production",
    "analyze": "npx vite build --mode production && npx vite-bundle-analyzer dist"
  },
