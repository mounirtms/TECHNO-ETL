{
  "name": "Techno-ETL-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite && npm run server",
    "backend": "node --inspect-brk backend/server.js",
    "server": "node backend/server.js",
    "authServer":"nodemon backend/authServer.js",
    "build": "vite build && npm run copy-backend && npm run copy-docs",
    "prebuild": "node scripts/updateVersion.js",
    "copy-backend": "xcopy /E /I /Y backend\\dist dist\\backend",
    "copy-docs": "xcopy /E /I /Y docs\\dist dist\\docs",
    "deploy": "firebase deploy --only hosting",
    "clean": "rm -rf node_modules/.vite && rm -rf dist && npm install && npm run dev",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.4",
    "@mui/lab": "^6.0.0-beta.27",
    "@mui/material": "^6.4.4",
    "@mui/x-data-grid": "^7.26.0",
    "@mui/x-date-pickers": "^7.26.0",
    "@mui/x-tree-view": "^7.26.0",
    "axios": "^1.7.8",
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "easy-soap-request": "^5.6.1",
    "express": "^4.18.2",
    "firebase": "^11.2.0",
    "framer-motion": "^11.12.0",
    "http-proxy-middleware": "^2.0.6",
    "msnodesqlv8": "^4.5.0",
    "mssql": "^11.0.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "nodemon": "^3.1.9",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hot-toast": "^2.4.1",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.15.0",
    "react-toastify": "^10.0.6",
    "recharts": "^2.8.0",
    "sass": "^1.81.0",
    "stylis": "^4.3.0",
    "stylis-plugin-rtl": "^2.1.1",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "vite": "^4.4.5"
  }
}