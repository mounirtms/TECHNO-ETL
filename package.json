    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^2.8.0",
    "sass": "^1.81.0",
    "serve-static": "^2.2.0",
    "stylis": "^4.3.0",
    "stylis-plugin-rtl": "^2.1.1",
    "xlsx": "^0.18.5"
