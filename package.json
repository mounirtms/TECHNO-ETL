  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 80",
    "start": "vite --host 0.0.0.0 --port 80",
    "backend": "node --inspect backend/server.js",
    "server": "node backend/server.js",
    "build": "vite build",
    "build:clean": "rimraf dist && npm run build",
    "build:prod": "npm run clean && npm run lint:fix && npm run build:frontend && npm run build:backend && npm run build:docs && npm run dist:combine",
    "build:frontend": "vite build",
    "build:backend": "cd backend && npm run build:prod",
    "build:docs": "cd docs && npm run build",
    "dist:combine": "node scripts/combine-dist.js",
    "preview": "vite preview --port 3000",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "clean": "rimraf dist coverage .nyc_output logs",
    "clean:all": "npm run clean && cd backend && npm run clean && cd ../docs && rimraf dist",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "start:full": "concurrently \"npm run server\" \"npm run start\"",
    "serve:prod": "cd dist/backend \u0026\u0026 npm install --production \u0026\u0026 npm start",
    "postinstall": "cd backend \u0026\u0026 npm install \u0026\u0026 cd ../docs \u0026\u0026 npm install"
  },
