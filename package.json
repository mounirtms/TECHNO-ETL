{
  "name": "Techno-ETL",
  "private": true,
  "version": "2.0.0",
  "author": {
    "name": "Mounir Abderrahmani",
    "email": "mounir.ab@techno-dz.com",
    "url": "https://mounir1.github.io"
  },
  "homepage": "https://mounir1.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/mounir/techno-etl.git"
  },
  "scripts": {
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "start": "vite --host 0.0.0.0 --port 80",
    "backend": "cd backend && npm start",
    "server": "cd backend && npm run dev",
    "build": "npm run type-check && vite build --mode production",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:debug": "vite build --mode development --minify false --sourcemap true",
    "preview": "vite preview --host 0.0.0.0 --port 80",
    "preview:build": "npm run build && npm run preview",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rimraf dist .tsbuildinfo node_modules/.vite",
    "clean:cache": "rimraf node_modules/.cache .eslintcache",
    "clean:all": "npm run clean && npm run clean:cache && rimraf node_modules",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",

    "backend:build": "cd backend && npm run build",
    "backend:start": "cd backend && npm start",
    "deploy": "npm run build && node deploy-optimized.js",
    "health-check": "curl -f http://localhost:80/health || exit 1",
    "update-deps": "npx npm-check-updates -u",
    "size-check": "npm run build && npx bundlesize",
    "postinstall": "npm run type-check",
    "info": "echo üöÄ TECHNO-ETL v3.0.0 - Modernized with TypeScript + Tailwind && echo üë®‚Äçüíª Author: Mounir Abderrahmani && echo üìß Email: mounir.ab@techno-dz.com",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
