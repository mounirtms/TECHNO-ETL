# Production Environment Configuration for Techno ETL

# API Configuration
VITE_API_BASE_URL=http://etl.techno-dz.com:5000
VITE_BACKEND_SERVER=http://etl.techno-dz.com:5000

# Magento Configuration
VITE_MAGENTO_BASE_URL=https://technostationery.com
VITE_MAGENTO_API_URL=https://technostationery.com/rest/V1

# Application Configuration
VITE_APP_NAME=Techno ETL
VITE_APP_VERSION=1.0.0
VITE_APP_DOMAIN=etl.techno-dz.com

# Database Configuration (for frontend display only)
VITE_SQL_CEGID_SERVER=10.0.2.58
VITE_SQL_CEGID_SERVER_DATABASE=Y2_LAB
VITE_SQL_CEGID_SERVER_USER=
VITE_SQL_CEGID_SERVER_PASSWORD=
VITE_SQL_CEGID_SERVER_INSTANCE=

# Security
VITE_ENABLE_HTTPS=true
VITE_SECURE_COOKIES=true

# Performance
VITE_ENABLE_COMPRESSION=true
VITE_CACHE_DURATION=300000

# Logging
VITE_LOG_LEVEL=error
VITE_ENABLE_ANALYTICS=true

# CORS Configuration
VITE_ALLOWED_ORIGINS=https://etl.techno-dz.com,https://techno-webapp.web.app

# User Agent
VITE_USER_AGENT=Techno-ETL/1.0.0 (etl.techno-dz.com)
