# TECHNO-ETL Development Environment Configuration
# Optimized for fast development and debugging

# Development Mode Settings
NODE_ENV=development
VITE_PORT=80
BACKEND_PORT=5000

# Performance Optimizations
ENABLE_RATE_LIMIT=false
VITE_FAST_DEV=true
DISABLE_LOGGING=false

# Database Settings (Optional for dev)
# REDIS_URL=redis://localhost:6379
# MDM_DB_CONNECTION_STRING=your_connection_string

# Frontend Settings
VITE_API_BASE_URL=http://localhost:5000
VITE_ENABLE_HMR=true
VITE_ENABLE_SOURCEMAPS=true

# Backend Settings
CORS_ORIGIN=http://localhost:80,http://127.0.0.1:80,http://localhost:3000
LOG_LEVEL=info

# Development Flags
DEV_SKIP_DB_INIT=false
DEV_FAST_STARTUP=true
DEV_ENABLE_CACHE=true