# Development Environment Configuration for Techno ETL

# API Configuration
VITE_API_BASE_URL=http://localhost:5000
VITE_BACKEND_SERVER=http://localhost:5000

# Magento Configuration
VITE_MAGENTO_BASE_URL=https://technostationery.com
VITE_MAGENTO_API_URL=https://technostationery.com/rest/V1

# Application Configuration
VITE_APP_NAME=Techno ETL
VITE_APP_VERSION=1.0.0
VITE_APP_DOMAIN=localhost:80

# Database Configuration (for frontend display only)
VITE_SQL_CEGID_SERVER=10.0.2.58
VITE_SQL_CEGID_SERVER_DATABASE=Y2_LAB
VITE_SQL_CEGID_SERVER_USER=
VITE_SQL_CEGID_SERVER_PASSWORD=
VITE_SQL_CEGID_SERVER_INSTANCE=

# Security
VITE_ENABLE_HTTPS=false
VITE_SECURE_COOKIES=false

# Performance
VITE_ENABLE_COMPRESSION=false
VITE_CACHE_DURATION=60000

# Logging
VITE_LOG_LEVEL=debug
VITE_ENABLE_ANALYTICS=false

# CORS Configuration
VITE_ALLOWED_ORIGINS=http://localhost:80,http://localhost:5000

# User Agent
VITE_USER_AGENT=Techno-ETL/1.0.0 (localhost:80)

# Development specific
VITE_NODE_ENV=development
VITE_DEBUG=true
